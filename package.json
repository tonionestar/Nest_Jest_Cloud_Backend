{
  "name": "@clippic/user-v2",
  "version": "0.0.0",
  "private": true,
  "main": "dist/app.js",
  "scripts": {
    "build": "tsoa spec-and-routes && tsc",
    "start": "node dist/app.js",
    "dev":  "npm run build && node dist/app.js",
    "test": "jest",
    "test:ci": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "seed": "export NODE_ENV=dev || set NODE_ENV=dev&& ts-node src/database/seed/seed.ts"
  },
  "dependencies": {
    "@clippic/clippic-errors": "2.2.0",
    "axios": "1.3.3",
    "cors": "2.8.5",
    "debug": "4.3.4",
    "email-templates": "11.0.3",
    "express": "4.18.2",
    "express-prom-bundle": "6.6.0",
    "firebase-admin": "11.5.0",
    "jaeger-client": "3.19.0",
    "jsonwebtoken": "9.0.0",
    "mysql": "2.18.1",
    "nodemailer": "6.9.1",
    "opentracing": "0.14.7",
    "prom-client": "14.1.1",
    "pug": "3.0.2",
    "swagger-ui-express": "4.6.0",
    "tsoa": "5.1.1",
    "typeorm": "0.3.12"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/email-templates": "10.0.1",
    "@types/express": "4.17.17",
    "@types/jaeger-client": "3.18.4",
    "@types/jest": "29.4.0",
    "@types/jsonwebtoken": "9.0.1",
    "@types/multer": "1.4.7",
    "@types/node": "18.13.0",
    "@types/nodemailer": "6.4.7",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/parser": "5.52.0",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "eslint": "8.34.0",
    "moment": "2.29.4",
    "multer": "1.4.5-lts.1",
    "jest": "29.4.2",
    "jest-junit": "15.0.0",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "maxWorkers": 1,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@App/(.*)$": "<rootDir>/src/$1"
    },
    "coverageReporters": ["text", "text-summary"],
    "collectCoverageFrom": ["src/routes/**/*.ts", "!**/node_modules/**"],
    "setupFiles": [
      "<rootDir>/jest/setEnvVars.js"
    ]
  }
}
