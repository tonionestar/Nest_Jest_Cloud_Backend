{
  "name": "@clippic/user-v2",
  "version": "0.0.0",
  "private": true,
  "main": "dist/app.js",
  "scripts": {
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "build": "tsoa spec-and-routes && tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:ci": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "seed": "export NODE_ENV=dev || set NODE_ENV=dev&& ts-node src/database/seed/seed.ts"
  },
  "dependencies": {
    "@clippic/clippic-errors": "2.2.0",
    "axios": "1.4.0",
    "cors": "2.8.5",
    "email-templates": "11.0.3",
    "express": "4.18.2",
    "express-prom-bundle": "6.6.0",
    "firebase-admin": "11.7.0",
    "jaeger-client": "3.19.0",
    "jsonwebtoken": "9.0.0",
    "mysql": "2.18.1",
    "nodemailer": "6.9.1",
    "opentracing": "0.14.7",
    "prom-client": "14.2.0",
    "pug": "3.0.2",
    "swagger-ui-express": "4.6.2",
    "tsoa": "5.1.1",
    "typeorm": "0.3.15"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/email-templates": "10.0.1",
    "@types/express": "4.17.17",
    "@types/jaeger-client": "3.18.4",
    "@types/jest": "29.5.1",
    "@types/jsonwebtoken": "9.0.2",
    "@types/multer": "1.4.7",
    "@types/node": "18.16.2",
    "@types/nodemailer": "6.4.7",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/parser": "5.59.1",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-formatter-gitlab": "4.0.0",
    "moment": "2.29.4",
    "multer": "1.4.5-lts.1",
    "jest": "29.5.0",
    "jest-junit": "16.0.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "nodemon": "2.0.22",
    "concurrently": "8.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "maxWorkers": 1,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@App/(.*)$": "<rootDir>/src/$1"
    },
    "coverageReporters": ["text", "text-summary"],
    "collectCoverageFrom": ["src/routes/**/*.ts", "!**/node_modules/**"],
    "setupFiles": [
      "<rootDir>/jest/setEnvVars.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.tsx"
    ]
  }
}
