{
  "name": "@clippic/user-v2",
  "version": "0.0.0",
  "private": true,
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "test": "jest",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@clippic/clippic-errors": "file:../clippic-errors",
    "axios": "0.27.2",
    "cors": "2.8.5",
    "debug": "4.3.4",
    "express": "4.18.1",
    "express-prom-bundle": "6.4.1",
    "firebase-admin": "10.2.0",
    "jaeger-client": "3.19.0",
    "jsonwebtoken": "8.5.1",
    "nodemailer": "6.7.5",
    "opentracing": "0.14.7",
    "prom-client": "14.0.1",
    "typeorm": "0.3.6"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jaeger-client": "3.18.3",
    "@types/jest": "27.5.1",
    "@types/jsonwebtoken": "8.5.8",
    "@types/supertest": "2.0.12",
    "jest": "28.1.0",
    "supertest": "6.2.3",
    "ts-jest": "28.0.3",
    "ts-node": "10.8.0",
    "typescript": "4.7.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@App/(.*)$": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "<rootDir>/jest/setEnvVars.js"
    ]
  }
}
