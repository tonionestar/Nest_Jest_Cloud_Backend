{
  "name": "@clippic/user-v2",
  "version": "0.0.0",
  "private": true,
  "main": "dist/app.js",
  "scripts": {
    "build": "tsoa swagger-and-routes;tsc;",
    "start": "node dist/app.js",
    "dev":  "npm run build; node dist/app.js",
    "test": "jest",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@clippic/clippic-errors": "2.1.0",
    "axios": "1.2.1",
    "cors": "2.8.5",
    "debug": "4.3.4",
    "email-templates": "10.0.1",
    "express": "4.18.2",
    "express-prom-bundle": "6.5.0",
    "firebase-admin": "11.3.0",
    "jaeger-client": "3.19.0",
    "jsonwebtoken": "8.5.1",
    "nodemailer": "6.8.0",
    "opentracing": "0.14.7",
    "prom-client": "14.1.0",
    "pug": "3.0.2",
    "swagger-ui-express": "4.6.0",
    "tsoa": "4.1.3",
    "typeorm": "0.3.11"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/email-templates": "10.0.1",
    "@types/express": "4.17.14",
    "@types/jaeger-client": "3.18.4",
    "@types/jest": "29.2.4",
    "@types/jsonwebtoken": "8.5.9",
    "@types/multer": "1.4.7",
    "@types/node": "18.11.11",
    "@types/nodemailer": "6.4.6",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/parser": "5.45.1",
    "@typescript-eslint/eslint-plugin": "5.45.1",
    "eslint": "8.29.0",
    "moment": "2.29.4",
    "multer": "1.4.5-lts.1",
    "jest": "29.3.1",
    "supertest": "6.3.2",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "maxWorkers": 1,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@App/(.*)$": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "<rootDir>/jest/setEnvVars.js"
    ]
  }
}
